{
  "name": "anioke",
  "version": "0.1.0",
  "private": true,
  "author": "Sketch",
  "description": "Karaoki your anime songs.",
  "homepage": ".",
  "main": "public/main.js",
  "dependencies": {
    "better-sqlite3": "^7.5.3",
    "express": "^4.18.1",
    "internal-ip": "^7.0.0",
    "libass-wasm": "^4.0.0",
    "node-fetch": "^3.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xml2js": "^0.4.23",
    "yt-dlp-wrap": "^2.3.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "build-electron && react-scripts build",
    "electron": "concurrently -k \"cross-env BROWSER=none npm start\" \"build-electron -d\" \"wait-on public/.build-electron-done http://127.0.0.1:3000 && electronmon .\"",
    "rebuild": "electron-rebuild && cp node_modules/libass-wasm/dist/js/subtitles-octopus-worker.* public/"
  },
  "electronmon": {
    "patterns": [
      "!cache/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "build-electron": "^1.0.5",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.6",
    "electron-rebuild": "^3.2.7",
    "electronmon": "^2.0.2",
    "react-scripts": "5.0.1",
    "wait-on": "^6.0.1"
  }
}
